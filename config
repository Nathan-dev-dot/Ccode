// m = mandatory / n = non mandatory
[columns]
m :type
m :size
n :constraints
n :check
n :default

[error_codes]
1 :Database error
2 :Configuration file error
3 :Invalid XML format (check the mandatory attributes, the foreign and primary keys, and the names of the tables, columns and database)
4 :Error in SQL file
5 :Not enough memory available
